		"@types/pako": "^2.0.0",
		"@types/three": "^0.155.0",
		"gh-pages": "^6.0.0",
		"marked": "^7.0.5",
		"pako": "^2.1.0",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-highlight": "^7.3.0",
		"three": "^0.155.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"zod": "^3.22.1"
	},
	"type": "module"
}
